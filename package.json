{
  "name": "update-project-item-status",
  "version": "0.0.1",
  "private": true,
  "description": "TypeScript action to change GitHub Projects(beta) items status",
  "main": "lib/main.js",
  "scripts": {
    "build": "tsc",
    "format": "prettier --write '**/*.ts'",
    "format-check": "prettier --check '**/*.ts'",
    "lint": "eslint src/**/*.ts",
    "package": "ncc build --source-map --license licenses.txt",
    "test": "jest --coverage",
    "all": "npm run build && npm run format && npm run lint && npm run package && npm test"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/kalgurn/update-project-item-status.git"
  },
  "keywords": [
    "actions",
    "node",
    "setup"
  ],
  "author": "Kostiantyn Kulbachnyi",
  "license": "MIT",
  "dependencies": {
    "@actions/core": "^1.6.0",
    "@actions/github": "^5.0.0"
  },
  "devDependencies": {
    "@types/node": "^16.10.5",
    "@typescript-eslint/eslint-plugin": "^5.23.0",
    "@typescript-eslint/parser": "^5.23.0",
    "@vercel/ncc": "^0.31.1",
    "eslint": "^7.32.0",
    "eslint-plugin-github": "^4.3.2",
    "eslint-plugin-jest": "^25.3.2",
    "jest": "^27.2.5",
    "js-yaml": "^4.1.0",
    "prettier": "2.6.2",
    "ts-jest": "^27.1.2",
    "typescript": "^4.4.4"
  }
}
